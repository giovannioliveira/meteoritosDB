bin/rails s

bin/rails g controller <name> <action> (eg: bin/rails generate controller welcome index)
    app/views/<name>/<action>.html.erb (eg: app/views/<name>/<action>.html.erb)

config/routes.rb

bin/rails g model <name> [<attr>:<type>]*
bin/rake db:migrate
bin/rails g active_admin:resource <model>

bin/rails g active_admin:assets
bin/rails g migration <name>